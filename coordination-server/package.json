{
  "name": "@tarot/coordination-server",
  "version": "0.1.0",
  "description": "AI Coordination Server for Claude Code and GitHub Copilot collaboration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "mcp": "tsx src/mcp-server.ts",
    "demo": "tsx src/demo.ts",
    "test": "vitest",
    "test:contracts": "vitest tests/contracts",
    "test:mocks": "vitest tests/mocks",
    "test:integration": "vitest tests/integration",
    "test:watch": "vitest --watch",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "clean": "rm -rf dist node_modules",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "coordination",
    "mcp",
    "claude",
    "copilot",
    "collaboration"
  ],
  "author": "TarotOutMyHeart Team",
  "license": "MIT"
}
